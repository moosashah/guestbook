# Development environment variables for compiler service
# Copy this to .env and fill in your actual AWS credentials

# AWS Configuration
AWS_REGION=eu-west-2
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
S3_BUCKET_NAME=guestbook-assets-2024-dev

# Service Configuration
PORT=3001

# FFmpeg Configuration (optional, for custom ffmpeg path)
FFMPEG_PATH=/usr/bin/ffmpeg
